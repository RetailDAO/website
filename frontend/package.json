{
  "name": "rd_crypto_dashboard_frontend",
  "version": "1.0.0",
  "description": "Retail DAO Terminal - Real-time cryptocurrency analytics and trading terminal frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --fix",
    "preview": "vite preview",
    "test": "vitest",
    "analyze": "vite-bundle-analyzer dist",
    "build:analyze": "npm run build && npm run analyze",
    "lighthouse": "lighthouse http://localhost:3000 --output json --output html --output-path ./lighthouse-report",
    "lighthouse:ci": "lhci autorun",
    "perf:test": "npm run build && npm run lighthouse",
    "perf:monitor": "npm run build:analyze && npm run lighthouse"
  },
  "keywords": [],
  "author": "Triple Tres",
  "license": "ISC",
  "dependencies": {
    "@tanstack/react-query": "^5.85.9",
    "@tanstack/react-query-devtools": "^5.85.9",
    "apexcharts": "^5.3.4",
    "lucide-react": "^0.542.0",
    "react": "^18.3.1",
    "react-apexcharts": "^1.7.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.2.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "lighthouse-ci": "^1.13.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "vite": "^7.1.3",
    "vite-bundle-analyzer": "^1.2.3",
    "vitest": "^3.2.4"
  }
}
